    <!--
        QuaternionBase Visualizer
    -->
    <Type Name="Vite::QuaternionBase&lt;*&gt;">
        <DisplayString>{{ W={w,g} | X={x,g} | Y={y,g} | Z={z,g} }</DisplayString>
        <Expand>
            <Item Name="W">w</Item>
            <Item Name="X">x</Item>
            <Item Name="Y">y</Item>
            <Item Name="Z">z</Item>
        </Expand>
    </Type>
    
